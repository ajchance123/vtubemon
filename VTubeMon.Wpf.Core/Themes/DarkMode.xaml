<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style TargetType="Grid">
        <Setter Property="Background" Value="#2B2031" />
    </Style>
    <Style TargetType="Grid" x:Key="HeaderGrid">
        <Setter Property="Background" Value="#705D97" />
    </Style>
    <Style TargetType="Grid" x:Key="ConsoleLog">
        <Setter Property="Background" Value="#472360" />
    </Style>
    <Style TargetType="GridSplitter">
        <Setter Property="Background" Value="#7F3EAC" />
    </Style>
    <Style TargetType="TabItem">
        <Setter Property="BorderBrush" Value="#9487FF" />
        <Setter Property="Background" Value="#4A3D64"/>
    </Style>
    <Style TargetType="TextBlock">
        <Setter Property="Foreground" Value="#A4ADD3" />
    </Style>
    <Style TargetType="TextBox">
        <Setter Property="BorderBrush" Value="#9487FF" />
        <Setter Property="Background" Value="#4A3D64" />
        <Setter Property="Foreground" Value="#A4ADD3" />
    </Style>
    <Style TargetType="TabControl">
        <Setter Property="Background" Value="#53559B" />
    </Style>
    <Style TargetType="Border">
        <Setter Property="BorderBrush" Value="#9487FF" />
    </Style>
</ResourceDictionary>